@model IEnumerable<MediationApplication.Models.MantraDto>
@{
    ViewBag.Title = "List";
}

<nav class="subNav">
    <a class="btn sessionBtn" href="/Mantra/New">Add Mantra</a>
</nav>

<h2 class="text-center">Mantras</h2>

<div>
    @foreach( var mantra in Model )
    {
        <h3><a class="card-title" href="/Mantra/Details/@mantra.MantraID">@mantra.MantraName</a></h3>
    }
</div>