@model IEnumerable<MediationApplication.Models.JournalEntryDto>
@{
    ViewBag.Title = "List";
}

<nav class="subNav">
    <a class="btn sessionBtn" href="/JournalEntry/New">Add Entry</a>
</nav>

<h2>Journal Entries</h2>

<div>
    @foreach( var entry in Model )
    {
        <div>
            <h3>Journal Entry for <a href="/JournalEntry/Details/@entry.JournalEntryID"><em>@entry.SessionDate.ToString("dddd, MMMM dd, yyyy")</em></a></h3>
        </div>
    }
</div>